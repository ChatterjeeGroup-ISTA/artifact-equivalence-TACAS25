
def main(){
    x:=12;
    n:=20;
    y:=0;
    cost:=0;
    for counter in [0..1000){
        if flip(7/10)==1 
        {x=x+1;}
        else 
        {x=x-1;}
        y = flip(1/4);
        if y==1 {y=1;} else {y=-1;}
        cost=cost + y;
    }
    observe(cost>=100);
    return (x,n,y,cost);
}