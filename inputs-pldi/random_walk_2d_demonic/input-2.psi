
def main(){
    x:=0;
    y:=0;
    r:=0;
    cost:=0;
    for counter in [0..1000){
        r=uniform(-2,1);
        if flip(4/10)==1
        {
            x=x+r;
        }
        else 
        {
            y=y+r;
        }
        cost=cost+x+y;
    }
    observe(cost>=100);
    return (x,y,cost,r);
}